<h1>Twitter</h1>
<% if flash[:notice] %>
<%= flash[:notice] %>
<% end %>
<% @tweets.each do |tweets| %>
<p>
<%= tweets.message %>
<%= tweets.created_at %>
<%= link_to 'destroy', tweet_path(tweets.id), method: :delete %>
<%= link_to 'show', tweet_path(tweets.id) %>
<%= link_to 'edit', edit_tweet_path(tweets.id) %>
</p>
<% end %>
<p><%= link_to 'new tweet', new_tweet_path %></p>